    "    answer = []\n",
    "    for i, v in enumerate(arr):\n",
    "        if i == 0:\n",
    "            answer.append(v)\n",
    "        elif v != arr[(i-1)]:\n",
    "            answer.append(v)\n",
    "    return answer"
